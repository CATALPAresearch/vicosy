{
  "name": "close-up-collab",
  "version": "1.0.0",
  "description": "synchronous video learning collaboration",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build -- prefix client"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
    "node_modules/**/node_modules"
    ],
    "delay": "2500",
    "env": {
      "NODE_ENV": "production",
      "NODE_CONFIG_DIR": "./config"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaHermes/close-up-collab.git"
  },
  "author": "Markus Hermes, Jost Fromhage",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MaHermes/close-up-collab/issues"
  },
  "homepage": "https://github.com/MaHermes/close-up-collab#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.12",
    "@teamwork/websocket-json-stream": "^2.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "color-hash": "^1.0.3",
    "concurrently": "^3.6.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "gravatar": "^1.6.0",
    "helmet": "^3.13.0",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.4",
    "on-change": "^0.1.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "quill": "^1.3.7",
    "rich-text": "^4.1.0",
    "sharedb": "^1.7.0",
    "sharedb-mongo": "^1.0.0-beta.16",
    "socket.io": "^2.4.1",
    "validator": "^10.4.0",
    "websocket-json-stream": "0.0.3",
    "winston": "^3.1.0",
    "ws": "^7.4.3"
  },
  "devDependencies": {
    "nodemon": "^1.18.3"
  }
}
